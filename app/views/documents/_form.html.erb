<%= form_with(model: @folder, url: folder_documents_path, method: :post) do |form| %>
  <div>
    <%= form.label 'File', style: "display: block" %>
    <%= form.file_field :documents, class: 'form-control' %>
  </div>

  <div class="mt-4">
    <%= form.submit 'Create document', class: 'btn btn-success' %>
  </div>
<% end %>
